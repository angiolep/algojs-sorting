{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },

  "plugins": ["plugins/markdown"],

  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "systemName": "algojs-sorting",
    "navType": "vertical"
  },
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true
  }
}
